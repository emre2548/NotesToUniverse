@model NoteListModel
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-md-3">
        @await Component.InvokeAsync("CategoryList")
    </div>
    <div class="col-md-9">
        @foreach (var item in Model.Notes)
        {
            <partial name="_notes" for="@item" />
        }
    </div>

</div>


